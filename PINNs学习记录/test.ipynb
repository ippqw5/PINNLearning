    "class MyAdd:\n",
    "    def __init__(self,x=0.0):\n",
    "        self.x = x\n",
    "    def __call__(self,a,b):\n",
    "        return a+b;\n",
    "    def __add__(self,a):\n",
    "        return self.x + a.x"
